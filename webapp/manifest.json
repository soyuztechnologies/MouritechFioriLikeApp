{
    "sap.app": {
		"_version": "1.3.0",
		"id": "emc.fin.ar",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
        },
        "i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{fruits}}"]
        },
        "dataSources": {
            "anubhavSystem":{
                "uri": "/sap/opu/odata/sap/ZODATA_AO_SRV/",
                "type": "OData",
                "settings": {"odataVersion": "2.0"}
            }
        },
		"ach": "FIN-ACC-AR"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_fiori3", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {
					"minVersion": ""
				},
				"sap.ui.layout": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
        "resources": {},
        "rootView":{
            "viewName": "emc.fin.ar.view.App",
            "type": "XML",
            "id": "idRootView"
        },
        "routing":{
            "config":{
                "viewPath": "emc.fin.ar.view",
                "viewType": "XML",
                "routerClass": "sap.m.routing.Router",
                "controlId": "idAppCon"
            },
            "routes": [{
				"pattern": "",
				"name": "start",
				"target": ["master", "empty"]
			}, {
				"pattern": "myfruits/{fruitId}",
				"name": "detail",
				"target": ["master", "modi"]
			},{
                "name": "supplier",
                "pattern": "MyFavouriteSupplierData",
                "target":["master", "supp"]
            }],
			"targets": {
				"master": {
					"viewName": "View1",
					"controlAggregation": "masterPages",
					"viewId": "idView1"
				},
				"empty": {
					"viewName": "Avengers",
					"controlAggregation": "detailPages",
					"viewId": "idEmpty"
				},
				"modi": {
					"viewName": "View2",
					"controlAggregation": "detailPages",
					"viewId": "idView2"
                },
                "supp":{
					"viewName": "Supplier",
					"controlAggregation": "detailPages",
					"viewId": "idSupplier"
                }
			}
        },
		"models": {
            "":{
                "dataSource": "anubhavSystem"
            },
			"fruits": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/fruitdata.json"
            },
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
		}
	},
	"_version": "1.35.0"
}