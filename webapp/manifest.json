{
    "sap.app": {
		"_version": "1.3.0",
		"id": "emc.fin.ar",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
        },
        "i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{fruits}}"]
		},
		"ach": "FIN-ACC-AR"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_fiori3", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {
					"minVersion": ""
				},
				"sap.ui.layout": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {},
		"models": {
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/fruitdata.json"
            },
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
		}
	},
	"_version": "1.35.0"
}